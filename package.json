{
  "name": "ConcertSpot",
  "version": "1.0.0",
  "description": "Find great events near you",
  "main": "index.js",
  "scripts": {
    "dev": "webpack -d --watch",
    "dev:serve": "NODE_ENV=serve webpack-dev-server --config webpack.config.js --hot --progress --colors --inline",
    "build": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "restart": "ssh ec2 'sudo service nginx restart; sudo supervisorctl restart all'",
    "push-backend": "rsync -ravzP --delete /Users/thomasbarrett/coding/concertspot/src/backend -e 'ssh -i /Users/thomasbarrett/coding/aws/ireland-key-pair.pem' ubuntu@52.17.9.164:/home/ubuntu/concertspot",
    "push-frontend": "rsync -ravzP -delete /Users/thomasbarrett/coding/concertspot/src/client/static -e 'ssh -i /Users/thomasbarrett/coding/aws/ireland-key-pair.pem' ubuntu@52.17.9.164:/home/ubuntu/concertspot",
    "push": "npm run push-backend && npm run push-frontend && npm run restart",
    "build:push": "npm run build && npm run push",
    "test-backend": "python -m unittest discover /Users/thomasbarrett/coding/concertspot/tests/server",
    "test-frontend": "mocha tests/client --compilers js:babel-register --recursive --require ignore-styles tests/client/dom-setup.js",
    "test": "npm run test-backend && npm run test-frontend",
    "test:watch": "npm test -- --watch"
  },
  "author": "Tom Barrett",
  "license": "ISC",
