{
  "name": "ConcertSpot",
  "version": "1.0.0",
  "description": "Find great events near you",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel build build-server redis",
    "dev:watch": "npm-run-all --parallel build:watch build-server:watch res",
    "dev:serve": "npm-run-all --parallel dev serve",
    "build": "webpack --config webpack.browser.config.js",
    "build:watch": "webpack --config webpack.browser.config.js --progress --colours --watch",
    "build:production": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "build-server": "webpack --config webpack.server.config.js",
    "build-server:watch": "webpack --config webpack.server.config.js --progress --colours --watch",
    "build-all": "npm-run-all -s build build-server",
    "build-all:production": "npm-run-all -s build:production build-server",
    "restart": "ssh ec2 'sudo service nginx restart; sudo supervisorctl restart all'",
    "push-backend": "rsync -ravzP --delete /Users/thomasbarrett/coding/concertspot/src/backend -e 'ssh -i /Users/thomasbarrett/coding/aws/ireland-key-pair.pem' ubuntu@52.17.9.164:/home/ubuntu/concertspot",
    "push-frontend": "rsync -ravzP -delete /Users/thomasbarrett/coding/concertspot/src/client/static -e 'ssh -i /Users/thomasbarrett/coding/aws/ireland-key-pair.pem' ubuntu@52.17.9.164:/home/ubuntu/concertspot",
    "push": "npm run push-backend && npm run push-frontend && npm run restart",
    "build-all:push": "npm run build-all:production && npm run push",
    "test-frontend": "istanbul cover _mocha -- tests/client --compilers js:babel-register --recursive --require ignore-styles tests/client/dom-setup.js",
    "test-server-mocha": "mocha tests/server/mocha --compilers js:babel-register --recursive --require ignore-styles",
    "test-server-webpack": "mocha-webpack --webpack-config webpack.test.config.babel.js --colors \"tests/server/webpack/**/*.test.js\"",
    "test-server": "npm run test-server-mocha && npm run test-server-webpack",
    "test": "npm-run-all -s test-server test-frontend",
    "test:watch": "npm test -- --watch",
    "serve": "node src/build/server.bundle.js",
    "redis": "redis-server"
  },
  "author": "Tom Barrett",
  "license": "ISC",
  "dependencies": {
    "async": "^2.1.4",
    "babel-loader": "^6.2.4",
    "css-loader": "^0.23.1",
    "env2": "^2.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "0.0.4",
    "google-map-react": "^0.22.0",
    "hapi": "^16.0.1",
    "html-webpack-plugin": "^2.22.0",
    "ignore-styles": "^4.0.0",
    "img-loader": "^1.3.1",
    "inert": "^4.0.3",
    "isomorphic-style-loader": "^1.1.0",
    "joi": "^10.0.5",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "md5": "^2.2.1",
    "moment": "^2.14.1",
    "net": "^1.0.2",
    "node-sass": "^3.8.0",
    "pg": "^6.1.1",
    "raw-loader": "^0.5.1",
    "react": "^15.2.1",
    "react-addons-css-transition-group": "^15.2.1",
    "react-addons-update": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-google-maps": "^4.11.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-redux": "^4.4.5",
    "react-responsive": "^1.1.4",
    "react-router": "^2.5.2",
    "react-widgets": "^3.4.5",
    "redis": "^2.6.3",
    "redux": "^3.5.2",
    "redux-form": "^6.0.5",
    "redux-persist": "^4.0.0-beta2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "url-loader": "^0.5.7",
    "vision": "^4.1.1",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.2",
    "expect": "^1.20.2",
    "inject-then": "^2.0.7",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.4.1",
    "mocha": "^2.5.3",
    "mocha-webpack": "^0.7.0",
    "nock": "^8.0.0",
    "npm-run-all": "^3.1.2",
    "react-addons-test-utils": "^15.3.2",
    "redux-mock-store": "^1.1.2",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.5.4"
  }
}
